    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "5.0.0-alpha.172",
    "@mui/material": "^5.16.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "formik": "^2.4.6",
    "notistack": "^3.0.1",
    "openapi-fetch": "^0.10.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "recoil": "^0.7.7",
    "swr": "^2.2.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "2.56.0",
    "@modern-js/app-tools": "2.56.0",
    "@modern-js/eslint-config": "2.56.0",
    "@modern-js/tsconfig": "2.56.0",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.7",
    "@types/react": "~18.0.26",
    "@types/react-dom": "~18.0.10",
    "lint-staged": "~13.1.0",
    "openapi-typescript": "^7.0.4",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "typescript": "~5.0.4"
  }
